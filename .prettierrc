{
  "semi": false,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 80,
  "endOfLine": "auto",
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "jsxBracketSameLine": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "importOrder": [
    "^pages/",
    "^api/",
    "^ui/",
    "^lib/",
    "^models/",
    "^helpers/",
    "^globals/",
    "^[./]"
  ],
  "importOrderSeparation": true
}
